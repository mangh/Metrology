{
  "$schema": "http://json.schemastore.org/template",
  "author": "mangh",
  "name": "Units of Measurement C# Library",
  "identity": "Mangh.Metrology.Units.CS",
  "shortName": "csunits",
  "classifications": [
    "library",
    "metrology",
    "unit",
    "measure"
  ],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "%PROJECT%",
  "symbols": {
    "NamespaceOption": {
      "type": "parameter",
      "datatype": "text",
      "description": "namespace for units of measurement (e.g. \"CALINE3.Metrology\")"
    },
    "Namespace": {
      "type": "generated",
      "generator": "coalesce",
      "datatype": "text",
      "parameters": {
        "sourceVariableName": "NamespaceOption",
        "fallbackVariableName": "name"
      },
      "replaces": "%NAMESPACE%"
    }
  }
}